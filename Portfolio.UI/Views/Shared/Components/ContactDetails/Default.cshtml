@model List<Contact>

<div class="col-md-6" data-aos="fade-left" data-aos-delay="300">
    @foreach (var item in Model)
    {
        <div class="mt-3 px-1">
        
        <div class="h5">@item.Title</div>
        <p>@item.Description</p>
    </div>
    <br />
    <br />
    <br />
    <div class="mt-53 px-1">
        <div class="row">
            <div class="col-sm-2">
                <div class="pb-1">Email:</div>
            </div>
            <div class="col-sm-10">
                <div class="pb-1 fw-bolder">@item.Mail</div>
            </div>
            <div class="col-sm-2">
                <div class="pb-1">Adres:</div>
            </div>
            <div class="col-sm-10">
                <div class="pb-1 fw-bolder">@item.Address</div>
            </div>
            <div class="col-sm-2">
                <div class="pb-1">Telefon:</div>
            </div>
            <div class="col-sm-10">
                <div class="pb-1 fw-bolder">@item.Phone</div>
            </div>
        </div>
    </div>
    }
</div>
    
    

